$('.content').html("<%= j render 'loading', user: @current_user %>")
$('.firefly').css('background-color', '#cce8e5')

function flashLoadingMessage() {
  $('.zen_loading').fadeIn(500);
  setTimeout(function() {
    $('.zen_loading').fadeOut(500);
  },1500)
}
flashLoadingMessage();

setTimeout(function() {
  $.getScript("/levels/zen<%= "?skip_interstitial=true" if @skip_interstitial %>")
},1000)
